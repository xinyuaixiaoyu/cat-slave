{
    "extends": [
        "plugin:react/recommended"
    ],
    "parser": "babel-eslint",
    "env": {
        "es6": true,
        "browser": true,
        "node": true,
        "commonjs": true,
    },
    "root": true,
    "parserOptions": {
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true,
            "modules": true,
            "experimentalObjectRestSpread": true
        }
    },
    "rules": {
        "prettier/prettier": "error"
    },
    "plugins": [
        "react",
        "prettier"
    ]
}